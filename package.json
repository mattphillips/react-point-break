{
  "name": "react-point-break",
  "version": "0.0.1",
  "description": "React CSS media queries with breakpoint render props Component and Provider. ðŸ”«",
  "main": "dist/index.js",
  "scripts": {
    "build": "NODE_ENV=production rollup --config rollup.config.js",
    "contributor": "all-contributors add",
    "contributor:gen": "all-contributors generate",
    "lint": "eslint src",
    "lint:fix": "yarn lint -- --fix",
    "prepublishOnly": "yarn build",
    "precommit": "lint-staged",
    "prettier": "prettier 'src/**/*.js' --write --single-quote=true --print-width=120",
    "test": "jest",
    "test:coverage": "yarn test -- --coverage",
    "test:report": "codecov",
    "test:watch": "yarn test -- --watch"
  },
  "keywords": [
    "react",
    "css",
    "media",
    "query",
    "queries",
    "breakpoint",
    "point break",
    "render props"
  ],
  "author": "Matt Phillips <matt@mattphillips.io> (mattphillips.io)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattphillips/react-point-break.git"
  },
  "bugs": {
    "url": "https://github.com/mattphillips/react-point-break/issues"
  },
  "homepage": "https://github.com/mattphillips/react-point-break#readme",
  "devDependencies": {
    "all-contributors-cli": "^4.10.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-jest-assertions": "^0.1.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-gwt": "^1.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "codecov": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.17.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-react": "^7.6.1",
    "husky": "^0.14.3",
    "jest": "^22.1.4",
    "jest-each": "^0.3.1",
    "jest-extended": "^0.6.0",
    "lint-staged": "^6.1.0",
    "prettier": "^1.10.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "rollup": "^0.55.3",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.0.2"
  },
  "lint-staged": {
    "*.js": [
      "yarn prettier",
      "git add"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "setupFiles": [
      "./setup/enzyme.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupTestFrameworkScriptFile": "jest-extended"
  },
  "babel": {
    "env": {
      "production": {
        "presets": [
          [ "env", { "loose": true, "modules": false } ],
          "react"
        ],
        "plugins": [
          "transform-class-properties",
          "transform-object-rest-spread"
        ]
      },
      "test": {
        "presets": [
          "env",
          "react"
        ],
        "plugins": [
          "transform-class-properties",
          "transform-object-rest-spread",
          "babel-jest-assertions",
          "gwt"
        ]
      }
    }
  },
  "dependencies": {
    "create-react-context": "^0.1.6"
  }
}
